P = 
Q =  
N = 
E = 3 
D = 

init:
	./Rsa.py -a init -b 1024

encrypt: text
	./Rsa.py -a encrypt -f text -o text.enc -n $(N) -k $(E)

decrypt: text.enc
	./Rsa.py -a decrypt -f text.enc -o text.dec -n $(N) -k $(D)

decryptCRT: text.enc
	./Rsa.py -a decrypt -f text.enc -o text.dec -n $(N) -k $(D) -P $(P) -Q $(Q) -E $(E)
